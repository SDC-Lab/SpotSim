package org.cloudbus.spotsim.cloud;


import org.cloudbus.spotsim.cloud.spothistory.SpotPriceRecord;


public class PriceChangeEvent {

    private final InstanceType type;
    private final SpotPriceRecord priceRecord;
	public PriceChangeEvent(InstanceType type, SpotPriceRecord priceRecord) {
		super();
		this.type = type;
		this.priceRecord = priceRecord;
	}
	public InstanceType getType() {
		return type;
	}
	public SpotPriceRecord getPriceRecord() {
		return priceRecord;
	}

}